{
    "name": "acmephp/ssl",
    "description": "PHP wrapper around OpenSSL extension providing SSL encoding, decoding, parsing and signing features",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/acmephp/ssl",
    "keywords": [
        "certificate",
        "https",
        "acmephp",
        "ssl",
        "RSA",
        "ECDSA",
        "openssl",
        "CSR",
        "x509"
    ],
    "authors": [
        {
            "name": "Titouan Galopin",
            "email": "galopintitouan@gmail.com",
            "homepage": "http://titouangalopin.com"
        },
        {
            "name": "Jérémy Derussé",
            "homepage": "https://twitter.com/jderusse"
        }
    ],
    "autoload": {
        "psr-4": {
            "AcmePhp\\Ssl\\": "."
        }
    },
    "require": {
        "php": ">=7.2.5",
        "ext-hash": "*",
        "ext-openssl": "*",
        "lib-openssl": ">=0.9.8",
        "webmozart/assert": "^1.0"
    },
    "config": {
        "sort-packages": true
    }
}
